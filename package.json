{
  "name": "blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "g": "hexo g",
    "start": "hexo g && hexo s",
    "draft": "hexo g && hexo s --draft",
    "build": "hexo generate",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "server": "hexo server",
    "publish": "hexo clean && hexo g && qshell qupload upload.conf && qshell cdnrefresh -i ./torefresh.txt",
    "publish2": "hexo clean && hexo g && qshell qupload2 --src-dir=D:/profile/blog/public --bucket=lfm-blog --overwrite --rescan_local && qshell cdnrefresh -i ./torefresh.txt"
  },
  "hexo": {
    "version": "6.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "babel-plugin-inferno": "^6.3.0",
    "bulma-stylus": "0.8.0",
    "hexo": "^6.0.0",
    "hexo-browsersync": "^0.3.0",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-index": "^2.0.0",
    "hexo-generator-tag": "^1.0.0",
    "hexo-githubcalendar": "^1.2.3",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-inferno": "^0.1.3",
    "hexo-renderer-marked": "^5.0.0",
    "hexo-renderer-stylus": "^2.0.1",
    "hexo-server": "^3.0.0",
    "hexo-theme-fluid": "^1.8.14",
    "nunjucks": "^3.2.3"
  }
}
